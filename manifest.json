{
  "manifest_version": 2,
  "name": "live sound effect",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "DJ sound"
  }
}